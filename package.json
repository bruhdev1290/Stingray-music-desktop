{
  "name": "stingray-music",
  "applicationId": "StingrayMusic",
  "productName": "Stingray Music",
  "version": "1.6.4",
  "description": "A music client for Stingray Music based on Electron and Vue.js written from scratch with performance in mind.",
  "license": "AGPL-3.0",
  "exports": "./build/index.js",
  "main": "./build/index.js",
  "author": "Stingray Music Team",
  "repository": "github:bruhdev1290/Stingray-music-desktop",
  "type": "module",
  "bugs": {
    "url": "https://github.com/bruhdev1290/Stingray-music-desktop/issues"
  },
  "homepage": "https://github.com/bruhdev1290/Stingray-music-desktop",
  "buildResources": "resources",
  "scripts": {
    "bootstrap": "npx -y check-engine && pnpm i",
    "build": "tsc && lessc --insecure ./src/renderer/style.less ./src/renderer/style.css",
    "start": "npm run build && electron ./build/index.js",
    "dist": "npm run build && electron-builder --publish=never",
    "dist:win": "npm run build && electron-builder --win --publish=never",
    "dist:linux": "npm run build && electron-builder --linux --publish=never",
    "dist:all": "npm run build && electron-builder -wl --publish=never",
    "dist:universalNotWorking": "npm run build && electron-builder --mac --universal --publish:never",
    "winget": "npm run build && electron-builder --win -c winget.json",
    "msft": "npm run build && electron-builder -c msft-package.json",
    "mstest": "npm run build && electron-builder -c msft-test.json",
    "steamdeck": "npm run build && electron-builder --linux -c steam-deck.json",
    "format:check": "npx prettier --check \"src/**/*.{js,json,ts,less}\"",
    "format:write": "npx prettier --write \"src/**/*.{js,json,ts,less}\""
  },
  "dependencies": {
    "@achingbrain/ssdp": "^4.2.1",
    "@crowdin/ota-client": "^1.1.3",
    "adm-zip": "^0.5.16",
    "airtunes2": "https://codeload.github.com/ciderapp/node_airtunes2/tar.gz/0c6abe067a1f388f6c95224c85d95fa181169bc8",
    "castv2-client": "https://codeload.github.com/ciderapp/node-castv2-client/tar.gz/acaaf2c26dc4f4f6764bd07df081166a508a76ab",
    "chokidar": "^3.6.0",
    "discord-auto-rpc": "^1.0.17",
    "ejs": "^3.1.10",
    "electron-log": "^5.2.4",
    "electron-notarize": "^1.2.2",
    "electron-store": "^9.0.0",
    "electron-updater": "6.2.1",
    "electron-window-state": "^5.0.3",
    "express": "^4.21.2",
    "get-port": "7.1.0",
    "jimp": "^0.22.12",
    "lastfmapi": "^0.1.1",
    "mdns-js": "https://codeload.github.com/ciderapp/node-mdns-js/tar.gz/a34c514981c4dded54ae47e49660a4497dfedf0c",
    "mpris-service": "https://codeload.github.com/ciderapp/mpris-service/tar.gz/ee4cce67814e3b955504e32666eb10c9a17ef443",
    "music-metadata": "7.14.0",
    "node-fetch": "^3.3.2",
    "node-ssdp": "^4.0.1",
    "qrcode": "^1.5.4",
    "request": "^2.88.2",
    "simple-ssdp": "^1.0.2",
    "source-map-support": "^0.5.21",
    "ssdp-js": "^1.0.1",
    "ts-md5": "1.3.1",
    "upnp-mediarenderer-client": "https://codeload.github.com/vapormusic/node-upnp-mediarenderer-client/tar.gz/d4856931500026eb6c4c67b3eb12c1c34755627e",
    "v8-compile-cache": "^2.4.0",
    "wallpaper": "7.2.1",
    "ws": "^8.18.0",
    "xml2js": "^0.6.2",
    "youtube-search-without-api-key": "^2.0.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/discord-rpc": "4.0.8",
    "@types/express": "^4.17.21",
    "@types/musickit": "https://codeload.github.com/ciderapp/musickit-types/tar.gz/2600a2c345cc30a9f94f69d85cc1e9a7430bb466",
    "@types/node": "^20.17.9",
    "@types/node-ssdp": "^4.0.4",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/request": "^2.48.12",
    "@types/upnp-mediarenderer-client": "^1.4.3",
    "@types/ws": "^8.5.13",
    "@types/xml2js": "^0.4.14",
    "electron": "github:castlabs/electron-releases#v32.2.6+wvcus",
    "electron-builder": "^24.13.3",
    "electron-builder-notarize-pkg": "^1.2.0",
    "electron-webpack": "^2.8.2",
    "less": "^4.2.1",
    "node-gyp": "^10.3.1",
    "prettier": "3.2.5",
    "typescript": "^5.7.2",
    "vue-devtools": "^5.1.4",
    "webpack": "~5.91.0"
  },
  "fileAssociations": [
    {
      "name": "Stingray Music",
      "mimeType": "x-scheme-handler/stingray;x-scheme-handler/music;",
      "schemes": [
        "stingray",
        "music"
      ],
      "protocols": [
        "stingray",
        "music"
      ]
    }
  ],
  "build": {
    "electronDownload": {
      "mirror": "https://github.com/castlabs/electron-releases/releases/download/v"
    },
    "appId": "stingray-music",
    "afterPack": "./resources/afterPack.cjs",
    "afterSign": "./resources/notarize.cjs",
    "protocols": [
      {
        "name": "Stingray Music",
        "schemes": [
          "stingray",
          "music"
        ]
      }
    ],
    "extends": null,
    "files": [
      "./build/**/*",
      "./src/**/*",
      "./resources/icons/**/*"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "synopsis": "A music client for Stingray Music. ",
      "category": "AudioVideo",
      "icon": "stingray-music",
      "executableName": "stingray-music"
    },
    "appx": {
      "applicationId": "StingrayMusic",
      "displayName": "Stingray Music",
      "identityName": "StingrayMusic",
      "publisherDisplayName": "Stingray Music Team",
      "backgroundColor": "transparent",
      "setBuildNumber": true
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE",
      "deleteAppDataOnUninstall": true,
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "pkg": {
      "installLocation": "/Applications",
      "background": {
        "file": "./resources/bg.png",
        "alignment": "bottomleft"
      },
      "allowAnywhere": true,
      "allowCurrentUserHome": true,
      "allowRootDirectory": true,
      "license": "./LICENSE",
      "isVersionChecked": false,
      "isRelocatable": false,
      "overwriteAction": "upgrade"
    },
    "snap": {
      "slots": [
        {
          "mpris": {
            "interface": "mpris"
          }
        }
      ]
    },
    "dmg": {
      "background": "./resources/bg.tiff",
      "icon": "resources/icons/icon.ico"
    },
    "mas": {
      "entitlements": "./resources/entitlements.mac.plist",
      "entitlementsInherit": "./resources/entitlements.mac.plist"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "resources/icons/icon.ico"
    },
    "directories": {
      "buildResources": ".",
      "output": "dist"
    },
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "icon": "./resources/icons/icon.icns",
      "category": "public.app-category.music",
      "entitlements": "./resources/entitlements.mac.plist",
      "entitlementsInherit": "./resources/entitlements.mac.plist",
      "darkModeSupport": true,
      "target": [
        "dmg"
      ],
      "extendInfo": {
        "NSUserNotificationAlertStyle": "alert"
      }
    }
  },
  "electronWebpack": {
    "devtool": "source-map"
  },
  "packageManager": "pnpm@9.15.0"
}
